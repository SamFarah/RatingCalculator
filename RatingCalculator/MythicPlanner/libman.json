{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [

    {
      "provider": "cdnjs",
      "library": "bootstrap@5.3.2",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "js/bootstrap.bundle.min.js",
        "css/bootstrap.min.css"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "popper.js@2.11.8",
      "destination": "wwwroot/lib/popper.js/",
      "files": [
        "umd/popper.min.js",
        "umd/popper.min.js.map"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "bootstrap-select@1.14.0-beta3",
      "destination": "wwwroot/lib/bootstrap-select/",
      "files": [
        "css/bootstrap-select.min.css",
        "js/bootstrap-select.min.js",
        "js/bootstrap-select.min.js.map"
      ]
    }
  ]
}